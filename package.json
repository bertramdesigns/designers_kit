{
  "name": "designers-kit",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate-licenses": "license-checker --relativeLicensePath --json > ./licenses.json",
    "build": "vite build",
    "serve": "vite preview",
    "tauri": "tauri",
    "appwrite:id": "node ./appwrite/scripts/random_id/generate_id.cjs",
    "appwrite:start": "docker compose -f appwrite/docker-compose.yml up -d --remove-orphans",
    "appwrite:stop": "docker compose -f appwrite/docker-compose.yml stop",
    "appwrite:uninstall": "docker compose -f appwrite/docker-compose.yml down -v",
    "appwrite:login": "appwrite login --endpoint 'http://localhost/v1'",
    "appwrite:init": "pnpm run appwrite:init:user && pnpm run appwrite:init:schema && pnpm run appwrite:init:db",
    "appwrite:init:user": "sh ./appwrite/scripts/init/init.sh",
    "appwrite:init:schema": "node ./appwrite/scripts/init/generate_schema.cjs",
    "appwrite:init:db": "node ./appwrite/scripts/init/appwrite_cli_push_schema.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "license": "MIT",
  "dependencies": {
    "@kobalte/core": "^0.13.7",
    "@solidjs/router": "^0.15.2",
    "@tanstack/solid-table": "^8.20.5",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-store": "~2",
    "appwrite": "^16.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk-solid": "^1.1.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.3",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/html": "^8.4.7",
    "@storybook/html-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "appwrite-cli": "^6.1.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "html-vite": "link:@types/@storybook/html-vite",
    "jsdom": "^25.0.1",
    "license-checker": "^25.0.1",
    "postcss": "^8.4.49",
    "solidui-cli": "^0.7.1",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.6",
    "vite-plugin-solid": "^2.11.0",
    "vitest": "^2.1.8"
  }
}